@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container d-flex justify-content-center align-content-center h-100">
    <div class="w-25 h-50 p-5 mt-5 border border-white rounded-2">
        <h1 class="display-4 pb-3">Login</h1>
        <div id="form" class="form">
            <div id="textBoxEmail" class="mb-3"></div>
            <div id="textBoxPassword" class="mb-3"></div>
            <div id="textBoxConfirmPassword" class="mb-3"></div>
            <div id="buttonLogin">Login</div>
        </div>
    </div>
</div>


@section Scripts
{
    <script src="~/assets/index.js" asp-append-version="true"></script>
}
